		"event",
		"proxy",
		"state",
		"forward",
		"pattern",
		"delegate",
		"observer",
		"delegation",
		"forwarding"
	],
	"devDependencies": {
		"@types/node": "<15",
		"@typescript-eslint/eslint-plugin": "<6",
		"@typescript-eslint/parser": "<6",
		"eslint": "<9",
		"eslint-config-alloy": "<5",
		"eslint-config-prettier": "<9",
		"prettier": "<3",
		"ts-node": "<11",
