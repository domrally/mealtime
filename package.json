	"types": "build/src/exports/exports.d.ts",
	"homepage": "https://domrally.github.io/peekeasy",
	"description": "tools for observing proxies in typescript & web assembly",
	"engines": {
		"node": "18"
	},
	"scripts": {
		"format": "npx eslint --fix && npx prettier --write .",
		"start": "npx typedoc",
		"test": "npm run lint && npm run unit && npm run integration",
		"lint": "npx eslint && npx prettier --check .",
		"unit": "npx ts-node src/tests/unit/unit.ts",
		"integration": "npx ts-node src/tests/integration/integration.ts"
